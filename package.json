{
  "name": "react-umami",
  "version": "0.1.0",
  "description": "React integration for Umami Analytics - A lightweight and privacy-focused alternative to Google Analytics",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "require": "./dist/client.js",
      "import": "./dist/client.esm.js",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ledjay/react-umami.git"
  },
  "keywords": [
    "react",
    "umami",
    "analytics",
    "tracking",
    "privacy",
    "gdpr-compliant",
    "lightweight",
    "typescript"
  ],
  "author": {
    "name": "ledjay",
    "url": "https://github.com/ledjay"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ledjay/react-umami/issues"
  },
  "homepage": "https://github.com/ledjay/react-umami#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "semantic-release": "^22.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ]
}
